{
  "name": "@atomist/demo-sdm",
  "version": "1.0.0",
  "description": "Atomist SDM to build and deploy demo projects",
  "author": "Atomist, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist-seeds/demo-sdm.git"
  },
  "keywords": [
    "atomist",
    "automation",
    "demo",
    "github",
    "kubernetes",
    "sdm"
  ],
  "homepage": "https://github.com/atomist-seeds/demo-sdm#readme",
  "bugs": {
    "url": "https://github.com/atomist-seeds/demo-sdm/issues"
  },
  "dependencies": {
    "@atomist/automation-client": "1.8.0-master.20191028235651",
    "@atomist/sdm": "1.8.0-dynamic-goals.20191023214836",
    "@atomist/sdm-core": "1.8.0-dynamic-goals.20191029003037",
    "@atomist/sdm-pack-analysis": "2.0.0-master.20190924210620",
    "@atomist/sdm-pack-build": "1.0.7-master.20191011105702",
    "@atomist/sdm-pack-docker": "2.0.3-master.20191004184845",
    "@atomist/sdm-pack-fingerprint": "5.0.1-master.20191006120505",
    "@atomist/sdm-pack-gcp": "0.1.0-master.20191018215140",
    "@atomist/sdm-pack-issue": "1.2.3-master.20190916025056",
    "@atomist/sdm-pack-k8s": "1.10.3-master.20191017202233",
    "@atomist/sdm-pack-spring": "2.0.2-master.20191017114359",
    "@atomist/slack-messages": "^1.1.1",
    "@types/app-root-path": "^1.2.4",
    "@types/dateformat": "^3.0.0",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "app-root-path": "^2.2.1",
    "dateformat": "^3.0.3",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@atomist/sdm-local": "1.2.2-master.20191011215150",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.2.2",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.0",
    "supervisor": "^0.12.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-git-info",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --config tslint.json --format verbose --project .",
    "lint:fix": "npm run lint -- --fix",
    "start": "atm-start",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.1.0",
    "npm": ">=5.0.0"
  }
}
